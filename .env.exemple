# EmailJS Configuration (Client-side email service)
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_emailjs_service_id
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
NEXT_PUBLIC_EMAILJS_AUTOREPLY_TEMPLATE_ID=your_autoreply_template_id
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Resend API Configuration (Server-side email service)
RESEND_API_KEY=your_resend_api_key

# Google Sheets Configuration (Optional)
NEXT_PUBLIC_GOOGLE_SHEETS_ID=your_google_sheets_id
NEXT_PUBLIC_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key

# Webhook Configuration (Alternative)
WEBHOOK_ENDPOINT=https://your-webhook-endpoint.com/contact

# Analytics (Optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Environment
NODE_ENV=development